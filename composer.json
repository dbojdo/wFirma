{
    "name": "webit/w-firma-api",
    "description": "wFirma.pl API",
    "type": "library",
    "license": "MIT",
    "keywords": ["sdk", "api", "wfirma", "w-firma"],
    "require": {
        "php": ">=7.1",
        "jms/serializer": "^1.0.0|^2.0.0|^3.0.0",
        "doctrine/annotations": "^1.14 || ^2.0",
        "kriswallsmith/buzz": "^1.0.0",
        "nyholm/psr7": "^1.0.0",
        "psr/log": "^1.0.2 || ^2.0 || ^3.0",
        "psr/simple-cache": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3.2",
        "monolog/monolog": "^1.10.0",
        "nette/php-generator": "^3.5.4",
        "fakerphp/faker": "^1.17.0"
    },
    "suggest": {
        "symfony/cache": "To use VatCodeIdResolver with caching feature"
    },
    "autoload": {
        "psr-4": {
            "Webit\\WFirmaSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\WFirmaSDK\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-master": "2.x-dev"
        }
    }
}
