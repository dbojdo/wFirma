{
    "name": "webit/w-firma-api",
    "description": "wFirma.pl API",
    "type": "library",
    "license": "MIT",
    "keywords": ["sdk", "api", "wfirma", "w-firma"],
    "require": {
        "php": ">=5.4,<=7.0",
        "ext-curl": "*",
        "jms/serializer": "^1.0.0",
        "kriswallsmith/buzz": ">=0.12.0,<0.17",
        "psr/log": "^1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.0",
        "fzaninotto/faker": "^1.7.1",
        "monolog/monolog": "^1.10.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\WFirmaSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\WFirmaSDK\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-master": "2.x-dev"
        }
    }
}
